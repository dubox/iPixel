{
	"pluginName": "iPixel",
	"description": "图片转像素画",
	"author": "dubox",
	"homepage": "https://github.com/dubox/",
	"main": "ui/index.html",
	"version": "1.1.0",
	"logo": "ui/img/logo.png",
	"features": [{
			"code": "main",
			"explain": "iPixel像素画",
			"cmds": ["像素画", "iPixel", "pixel","图片",
				{
					"type": "files",
					"label": "iPixel",
					"fileType": "file",
					"match": "/\\.(jpg|png|jpeg)/i",
					"minLength": 1,
					"maxLength": 1
				},
				{
					"type": "img",
					"label": "iPixel"
				}]
		}

	],
	"preload": "preload.js"
}